{
  "displayName": "GithubVideoCompressor",
  "name": "github_video_compressor_app",
  "description": "A seamless local video compressing solution when upload videos to Github",
  "version": "0.0.1",
  "main": "./node_modules/.dev/main/index.js",
  "resources": "src/resources",
  "author": {
    "name": "Dominic Cicilio"
  },
  "license": "MIT",
  "scripts": {
    "start": "electron-vite preview",
    "dev": "npx ts-node ./dev_server.ts",
    "dev:electron": "electron-vite dev",
    "dev:launcher": "./launcher/dev_mode_launcher.py",
    "build:launcher": "pyinstaller --distpath=./launcher/dist ./launcher/dev_mode_launcher.py",
    "compile:app": "electron-vite build",
    "compile:packageJSON": "node ./bin/modules/postbuild/index.js",
    "prebuild": "run-s compile:app compile:packageJSON",
    "build": "./node_modules/.bin/electron-builder",
    "postinstall": "run-s prebuild install:deps",
    "install:deps": "electron-builder install-app-deps",
    "make:release": "node ./bin/modules/release/index.js",
    "release": "electron-builder --publish always"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "electron-router-dom": "^1.0.5",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "ssh2": "^1.15.0",
    "uuid": "^9.0.1",
    "ws": "^8.15.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/chokidar": "^2.1.3",
    "@types/cors": "^2.8.17",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.2",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/ssh2": "^1.11.18",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "cross-env": "^7.0.3",
    "electron": "^27.1.3",
    "electron-builder": "^24.9.1",
    "electron-extension-installer": "^1.2.0",
    "electron-vite": "^1.0.27",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "open": "8.4.2",
    "prettier": "^3.0.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "sass": "^1.64.2",
    "semver": "^7.5.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --quiet --fix"
    ]
  },
  "eslintIgnore": [
    "dist"
  ]
}
